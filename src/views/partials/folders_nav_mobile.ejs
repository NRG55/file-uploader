<nav aria-label="Folders navigation" class="flex mb-8 md:hidden ">
    <ul class="">
        <% if (locals.folderWithParentFoldersArray) {
            folderWithParentFoldersArray.forEach((element, index) => { %>
                <a href="/storage/<%= element.id %>" class="float-left py-2">
                    <% if (folderWithParentFoldersArray.length > 1 && index > 0) { %>
                        <span class="text-gray-400 mx-2"><</span>
                    <% } %>
                    <li class="inline-flex text-sm text-gray-400 <%= index === folderWithParentFoldersArray.length - 1 ? 'underline underline-offset-6' : '' %>
                        hover:text-white"
                    >                       
                        <%= element.name %>                                               
                    </li>
                </a>
        <% })} %>            
        <li><span aria-current="page"></span></li>
    </ul>
</nav>